<AnchorPane prefHeight="448.0" prefWidth="874.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.votingapp.VotingForm">
   <children>
      <ComboBox fx:id="queryComboBox" layoutX="608.0" layoutY="78.0" onAction="#queryComboBox_IndexChanged" prefHeight="25.0" prefWidth="236.0" />
      <Button fx:id="approveButton" layoutX="778.0" layoutY="115.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="65.0" text="Approve" />
      <Button fx:id="disapproveButton" layoutX="695.0" layoutY="115.0" mnemonicParsing="false" prefWidth="76.0" text="Disapprove" />
      <Button fx:id="backButton" layoutX="530.0" layoutY="408.0" mnemonicParsing="false" onAction="#backButton_Click" prefHeight="25.0" prefWidth="61.0" text="Back" />
      <TableView fx:id="table" layoutX="7.0" layoutY="8.0" prefHeight="428.0" prefWidth="510.0">
        <columns>
          <TableColumn fx:id="idCol" prefWidth="65.19997978210449" text="Query ID" />
          <TableColumn fx:id="textCol" prefWidth="443.59999084472656" text="Text Preview" />
        </columns>
      </TableView>
      <Label layoutX="528.0" layoutY="82.0" text="Select query" />
      <Label layoutX="528.0" layoutY="42.0" text="Please select a topic from the combo box and vote...">
         <font>
            <Font name="Franklin Gothic Demi" size="14.0" />
         </font>
      </Label>
      <TextArea fx:id="textArea" editable="false" layoutX="530.0" layoutY="170.0" prefHeight="230.0" prefWidth="320.0" wrapText="true" />
   </children>
</AnchorPane>
